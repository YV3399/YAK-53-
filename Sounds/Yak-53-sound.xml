  </windOpen>
  

  <stall>
   <name>stall</name>
   <path>Aircraft/Yak-53/Sounds/Yak-53_stall.wav</path>
   <condition>
    <or>
     <greater-than>
      <property>fdm/jsbsim/aero/alpha-deg</property>
      <value>15.0</value>
     </greater-than>
     <less-than>
      <property>fdm/jsbsim/aero/alpha-deg</property>
      <value>-10.0</value>
     </less-than>
    </or>
   </condition>
   <volume>
     <offset>-1.0</offset>
   </volume>
  </stall>

  
  <maxG>
    <name>MaxMinG</name>
    <mode>once</mode>
    <path>Aircraft/Yak-53/Sounds/Yak-53_maxG.wav</path>
    <condition>
      <or>
	<greater-than>
	  <property>fdm/jsbsim/accelerations/n-pilot-z-norm</property>
	  <value>2.5</value>
	</greater-than>
	<less-than>
	  <property>fdm/jsbsim/accelerations/n-pilot-z-norm</property>
	  <value>-5.0</value>
	</less-than>
      </or>
    </condition>
    <position>
      <x>1.0</x>
      <y>0.0</y>
      <z>-0.50</z>
    </position>
  </maxG>

  
  
  
  
  
