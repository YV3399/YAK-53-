   <name>windOpen</name>
   <mode>looped</mode>
   <path>Aircraft/Yak-53/Sounds/Yak-53_wind.wav</path>
   <property>sim/multiplay/generic/float[15]</property>
   <condition>
   <property>sim/multiplay/generic/float[0]</property>
