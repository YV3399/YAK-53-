<!-- ============== gear ================ -->

<click>
 <name>gearDown</name>
 <path>Aircraft/Yak-53/Sounds/Yak-53_gear.wav</path>
   <condition>
   <property>controls/gear/gear-down</property>
 </condition>   
 <volume>
 <factor>0.75</factor>
 </volume>
   <position>
    <x>1.0</x>
    <y>0.0</y>
    <z>-0.80</z>
   </position>
</click>

<click>
 <name>gearUp</name>
 <path>Aircraft/Yak-53/Sounds/Yak-53_gear.wav</path>
   <condition>
   <and>
   <not>
   <property>controls/gear/gear-down</property>
   </not>
	 <property>sim/multiplay/generic/int[9]</property>   
   </and>
 </condition>   
 <volume>
 <factor>0.75</factor>
 </volume>
   <position>
    <x>1.0</x>
    <y>0.0</y>
    <z>-0.80</z>
   </position>
</click>


<gearL>
 <name>gear lock</name>
 <mode>once</mode>
 <path>Aircraft/Yak-53/Sounds/Yak-53_lock.wav</path>
   <condition>
   <and>
     <less-than>
      <property>gear/gear/position-norm</property>
      <value>0.02</value>
     </less-than>
	 <property>sim/multiplay/generic/int[9]</property> 
	 </and>
   </condition>
 <volume>
 <factor>0.5</factor>
 </volume>
   <position>
    <x>1.0</x>
    <y>0.0</y>
    <z>-0.80</z>
   </position>
</gearL>

<gearL2>
 <name>gear lock2</name>
 <mode>once</mode>
 <path>Aircraft/Yak-53/Sounds/Yak-53_lock.wav</path>
   <condition>
