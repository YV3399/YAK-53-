      <property>gear/gear/position-norm</property>
      <value>0.98</value>
     </greater-than>
   </condition>
 <volume>
 <factor>0.35</factor>
 </volume>
   <position>
    <x>1.0</x>
    <y>0.0</y>
    <z>-0.80</z>
   </position>
</gearL2>

<gearC>
 <name>gear crash</name>
 <mode>once</mode>
 <path>Aircraft/Yak-53/Sounds/Yak-53_gear-crash.wav</path>
