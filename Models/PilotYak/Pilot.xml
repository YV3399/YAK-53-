<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>PilotYakSub.ac</path>

  <animation>
     <type>select</type>
    <object-name>Tors</object-name>
    <object-name>Lock</object-name>
    <object-name>Korpus</object-name>
    <object-name>Arm1L</object-name>
    <object-name>Arm2L</object-name>
    <object-name>Arm3L</object-name>
    <object-name>Arm1R</object-name>
    <object-name>Arm2R</object-name>
    <object-name>Arm3R</object-name>
    <object-name>Leg1L</object-name>
    <object-name>Leg2L</object-name>
    <object-name>BootL</object-name>
    <object-name>Leg1R</object-name>
    <object-name>Leg2R</object-name>
    <object-name>BootR</object-name>
    <object-name>Harness</object-name>
    <condition>
	<or>
	<and>
     <greater-than>	
     <property>sim/multiplay/generic/int[4]</property>
     <value>0</value>
     </greater-than>	 
     <less-than>	 
     <property>sim/multiplay/generic/int[4]</property>
     <value>8</value>
     </less-than>	 
     </and>
	 <equals>
     <property>sim/multiplay/generic/int[4]</property>
     <value>10</value>
	 </equals>
	 </or>
  </condition>
  </animation>
  

   <animation>
     <type>select</type>
    <object-name>Head</object-name>	 
    <condition>
	<and>
     <greater-than>	
     <property>sim/multiplay/generic/int[4]</property>
     <value>0</value>
     </greater-than>	 
     <less-than>	 
     <property>sim/multiplay/generic/int[4]</property>
     <value>8</value>
     </less-than>	 
     </and>     
  </condition>
  </animation>

  

  
  <animation>
    <type>rotate</type>
    <object-name>Head</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> -12 </factor> 
    <center>
      <x-m>  0.40 </x-m>
      <y-m>  0.00 </y-m>
      <z-m>  0.50 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Head</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>-25</factor>
    <center>
      <x-m>  0.40 </x-m>
      <y-m>  0.00 </y-m>
      <z-m>  0.50 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 0.000 </y>
      <z> 1.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Head</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor> -10.0</factor>
    <center>
      <x-m>  0.40 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.50 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation> 

  
<!-- Feet animations right -->
