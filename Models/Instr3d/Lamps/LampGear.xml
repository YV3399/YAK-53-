<PropertyList>
    <path>LampGr.ac</path>

  <animation>
  <object-name>LampSelR0</object-name> 
  <type>select</type> 
  <condition>
  <and>
  <greater-than>
    <property>systems/electrical/outputs/bus</property>
    <value>21</value>
   </greater-than>
   <equals>
   <property>controls/circuit-breakers/gear</property>
   <value>true</value>
   </equals>
   <equals>
   <property>control/gear-not-crash</property>
   <value>true</value>
   </equals>
  <or>
  <equals>
    <property>controls/lamp-test</property>
    <value>1</value>
   </equals>
  <equals>
  <property>controls/gear/gear-down</property> 
  <value>0</value> 
   </equals>
  </or>
  </and>
  </condition>
  </animation>
  

