<PropertyList>

  <path>Fuel.ac</path>
  
  <animation>
    <type>pick</type>
    <object-name>FuelTestPB</object-name>
    <visible>true</visible>
  <action>                             
   <desc>Fuel Lamp Test</desc>
   <button>0</button>               
   <binding>
      <command>property-assign</command>
      <property>control/fueltest</property>      
      <value>1</value>                 
   </binding>
   <mod-up>
    <binding>
      <command>property-assign</command>
      <property>control/fueltest</property>      
      <value>0</value>
    </binding>
   </mod-up>      
  </action>
 </animation>    

 <animation>
  <type>translate</type>
  <object-name>FuelTestPB</object-name>
  <property>control/fueltest</property>
  <factor>-0.004</factor>
  <axis>
