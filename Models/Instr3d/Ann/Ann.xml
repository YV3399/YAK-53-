<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>Ann.ac</path>


  <animation> 
    <type>select</type>
    <object-name>Ann1R</object-name>
    <condition>
  <and>
  <greater-than>
    <property>systems/electrical/outputs/bus</property>
    <value>21</value>
   </greater-than>
   <equals>
   <property>controls/circuit-breakers/ann</property>
   <value>true</value>
   </equals>
  <or>
  <equals>
    <property>controls/lamp-test</property>
    <value>1</value>
   </equals>
  <greater-than>
  <property>fdm/jsbsim/accelerations/n-pilot-z-norm[0]</property> 
  <value>7</value> 
   </greater-than>
  <less-than>
  <property>fdm/jsbsim/accelerations/n-pilot-z-norm[0]</property> 
  <value>-5</value> 
   </less-than>
  </or>
  </and>
  </condition>
 </animation>   
  
  <animation> 
    <type>select</type>
    <object-name>Ann2R</object-name>
    <condition>
  <and>
  <greater-than>
    <property>systems/electrical/outputs/bus</property>
    <value>21</value>
   </greater-than>
   <equals>
   <property>controls/circuit-breakers/ann</property>
   <value>true</value>
   </equals>
   <or>
  <equals>
    <property>controls/lamp-test</property>
    <value>1</value>
   </equals>
    <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>975</value>
   </less-than>
   <equals>
   <property>controls/circuit-breakers/generator</property>
   <value>false</value>
   </equals>
    </or>
   </and>
  </condition>
 </animation>   

  <animation> 
    <type>select</type>
    <object-name>Ann3Y</object-name>
    <condition>
  <and>
  <greater-than>
    <property>systems/electrical/outputs/bus</property>
    <value>21</value>
   </greater-than>
   <equals>
   <property>controls/circuit-breakers/ann</property>
   <value>true</value>
   </equals>
  <or>   
  <equals>
    <property>controls/lamp-test</property>
    <value>1</value>
   </equals>
  <and>
  <greater-than>
    <property>engines/engine/cht-degf</property>
    <value>545</value>
   </greater-than>
  <greater-than>
    <property>engines/engine/oil-temperature-degf</property>
    <value>203</value>
   </greater-than>
   </and>
  <and>
  <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>2850</value>
   </greater-than>
  <less-than>
    <property>engines/engine/oil-temperature-degf</property>
    <value>104</value>
   </less-than>
   </and>
  </or>
   </and>  
  </condition>
 </animation>   
  
  <animation> 
    <type>select</type>
    <object-name>Ann4G</object-name>
    <condition>
  <and>
  <greater-than>
    <property>systems/electrical/outputs/bus</property>
    <value>21</value>
   </greater-than>
   <equals>
   <property>controls/circuit-breakers/ann</property>
   <value>true</value>
   </equals>
   <or>
  <equals>
    <property>controls/lamp-test</property>
    <value>1</value>
   </equals>
  <equals>
    <property>controls/circuit-breakers/pitot-heat</property>
    <value>true</value>
   </equals>
   </or>
   </and>
  </condition>
 </animation>   
 

</PropertyList>
